{
    "name": "3x1io/filament-translations",
    "description": "Translations With DB & UI",
    "version": "dev-0.2.0",
    "keywords": [
        "laravel",
        "translations",
        "ui",
        "filament"
    ],
    "homepage": "https://3x1.io",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "io3x1\\FilamentTranslations\\": "src/"
        }
    },
    "authors": [
        {
            "name": "3x1",
            "email": "info@3x1.io",
            "role": "CEO"
        },
        {
            "name": "Timo Railo",
            "email": "timo@east.fi",
            "role": "Codebender"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "filament/notifications": "^3.0-stable"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "io3x1\\FilamentTranslations\\FilamentTranslationsProvider"
            ]
        }
    },
    "prefer-stable": true
}
